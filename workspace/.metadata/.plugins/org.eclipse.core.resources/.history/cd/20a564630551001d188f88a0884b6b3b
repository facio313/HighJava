public class UdpSender {
	private DatagramSocket ds;
	private DatagramPacket dp;
	
	private byte[] msg; // 패킷 송수신을 위한 바이트배열 선언
	
	public UdpSender(int port) {
		try {
			ds = new DatagramSocket(8888);
		}
	}
}
